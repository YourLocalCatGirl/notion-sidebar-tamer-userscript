{
  "manifest_version": 3,
  "name": "Notion Siderbar Tamer",
  "description": "Base Level Extension",
  "version": "0.1",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://www.notion.so/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "images/128.png"
  }
}
